{
	"scripts": {
		"frontend:install": "cd udagram-frontend && npm install --save --legacy-peer-deps",
		"backend:install": "cd udagram-api && npm install",
		"frontend:build":"cd udagram-frontend && export NODE_OPTIONS=--openssl-legacy-provider && npm run build",
		"backend:build":"cd udagram-api && npm run build",
		"tests":"cd udagram-frontend && npm run test",
		"frontend:deploy": "cd udagram-frontend && npm run deploy",
		"backend:deploy": "cd udagram-api && npm run deploy"
	}
}
